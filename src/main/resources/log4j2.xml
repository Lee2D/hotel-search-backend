<Configuration status="WARN">
    <Appenders>
        <Lambda name="main">
            <PatternLayout>
                <pattern>%d{yyyy-MM-dd HH:mm:ss} %X{AWSRequestId} %-5p %c{1} - %m%n</pattern>
            </PatternLayout>
        </Lambda>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="main"/>
        </Root>
    </Loggers>
</Configuration>